






@model Setting



<div class="container pt-5 pb-5">

    <div class="container">
        <h1 style="text-align:center">Update Settings</h1>
    </div>

    <div>


        <form method="post">
            
            <div class="form-group">
                <label asp-for="Value">@Model.Key</label>
                <input asp-for="Value" class="form-control"  placeholder="Value">
                <span asp-validation-for="Value" class="text-danger"></span>
            </div>
           
            <button type="submit" class="btn btn-primary">Update</button>
            <a class="btn btn-danger" asp-action="index" asp-controller="setting">Cancel</a>
        </form>


    </div>
</div>


